            </intent-filter>
        </activity>
        <activity android:name=".Controller.SignUpActivity"></activity>
        <activity android:name=".Controller.HomeActivity">
        </activity>
        <activity android:name=".Controller.SearchActivity"></activity>
        <activity android:name=".Controller.AnUongActivity"></activity>
        <activity android:name=".Controller.VuiChoiActivity"></activity>
